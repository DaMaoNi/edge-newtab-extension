{
  "manifest_version": 3,
  "name": "Moment",
  "version": "1.0.0",
  "description": "每次打开新标签页时显示一张随机图片背景",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["search"],
  "host_permissions": [
    "https://picsum.photos/*"
  ]
}
